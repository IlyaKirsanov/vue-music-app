{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"],
      "@components/*": ["src/components/*"],
      "@views/*": ["src/views/*"],
      "@assets/*": ["src/assets/*"],
      "@styles/*": ["src/styles/*"],
      "@config/*": ["src/config/*"],
      "@constants": ["src/constants"],
      "@utils/*": ["src/utils/*"],
      "@routing/*": ["src/router/*"],
      "@store/*": ["src/store/*"]
    }
  },
  "exclude": ["node_modules", "dist"]
}
